{
  "name": "swisnl/guzzle-fixture-handler",
  "description": "Fixture handler for Guzzle 6+",
  "type": "library",
  "keywords": [
    "swisnl",
    "swis",
    "guzzle",
    "fixture",
    "client",
    "psr-7"
  ],
  "homepage": "https://github.com/swisnl/guzzle-fixture-handler",
  "license": "MIT",
  "authors": [
    {
      "name": "Jasper Zonneveld",
      "email": "jasper@swis.nl",
      "homepage": "https://www.swis.nl/over-ons/jasper-zonneveld",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "guzzlehttp/guzzle": "^6.0|^7.0",
    "swisnl/php-http-fixture-client": "^2.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "guzzlehttp/psr7": "^1.7|^2.0",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Swis\\Guzzle\\Fixture\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4" : {
      "Swis\\Guzzle\\Fixture\\Tests\\" : "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "php-cs-fixer fix --dry-run -v",
    "fix-style": "php-cs-fixer fix"
  },
  "config": {
    "sort-packages": true
  }
}
